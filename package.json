{
  "repository": "https://github.com/jpb06/effect-github-stats.git",
  "main": "dist/index.js",
  "name": "effect-github-stats",
  "version": "1.0.9",
  "author": "jpb06 <jp.bois.06@outlook.fr>",
  "description": "Getting user stats from github",
  "keywords": [],
  "license": "MIT",
  "engines": {
    "node": "20.x"
  },
  "dependencies": {
    "@octokit/core": "^6.1.2",
    "chalk": "^5",
    "dotenv-flow": "^4.1.0",
    "effect": "^3.4.7",
    "effect-errors": "^1.4.11",
    "ts-pattern": "^5.2.0"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin": "^2.3.0",
    "@types/eslint": "^8.56.10",
    "@types/node": "^20.14.10",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "@vitest/coverage-v8": "^1.6.0",
    "copyfiles": "^2.4.1",
    "del-cli": "^5.1.0",
    "eslint": "<9",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-markdown": "^5.0.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vitest": "^0.5.4",
    "prettier": "^3.3.2",
    "readme-package-icons": "^1.1.14",
    "typescript": "^5.5.3",
    "vitest": "^1.6.0",
    "vitest-mock-extended": "^1.3.1"
  },
  "scripts": {
    "update-deps": "bunx npm-check-updates --root --format group -i",
    "copy-package": "copyfiles package.json ./dist/",
    "copy-readme": "copyfiles README.md ./dist/",
    "build": "del-cli ./dist && tsc --project tsconfig.prod.json",
    "postbuild": "bun run copy-package && bun run copy-readme",
    "lint": "eslint ./src",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test-ci": "vitest --coverage --run",
    "test-dev": "vitest --coverage",
    "test-coverage": "vitest run --coverage",
    "sync-icons": "pnpm generateReadmeIcons -h 50"
  }
}
